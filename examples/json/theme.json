	"styles": {
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "calc( var(--wp--custom--element-spacing) / 2)",
					"margin": {
						"top": "var:custom|element-spacing"
					}
				},
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/cover": {
				"spacing": {
					"margin": {
						"bottom": "0"
					},
					"padding": {
						"bottom": "calc( var(--wp--custom--element-spacing) * 4)",
						"left": "var:custom|site-spacing",
						"right": "var:custom|site-spacing",
						"top": "calc( var(--wp--custom--element-spacing) * 4)"
					}
				}
			},
			"core/heading": {
				"color": {
					"text": "var:preset|color|primary"
				},
				"spacing": {
					"margin": {
						"bottom": "0"
