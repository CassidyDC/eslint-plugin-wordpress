						"bottom": "0",
						"top": "var:custom|element-spacing"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "var:custom|line-height|medium"
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						"border": {
							"radius": "var:custom|default-border-radius"
						},
						"color": {
							"background": "var:preset|color|gray-light",
							"text": "var:preset|color|gray-dark"
						},
						"spacing": {
							"padding": {
								"bottom": "0.125rem",
								"left": "0.5rem",
								"right": "0.5rem",
								"top": "0.125rem"
							}
						},
						"typography": {
							"fontSize": "var:preset|font-size|x-small",
							"lineHeight": "var:custom|line-height|x-small"
						}
					}
				}
			},
			"core/quote": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "var:custom|element-spacing"
					},
					"padding": {
						"left": "calc( var(--wp--custom--element-spacing) / 1.5)"
					}
				}
			},
			"core/read-more": {
				"border": {
					"radius": "var:custom|default-border-radius"
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|primary-white"
				},
				"spacing": {
					"margin": {
						"top": "var:custom|element-spacing"
					},
					"padding": {
						"bottom": "0.4375rem",
						"left": "0.875rem",
						"right": "0.875rem",
						"top": "0.4375rem"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"fontWeight": "500",
					"lineHeight": "var:custom|line-height|small"
				}
			},
			"core/social-links": {
				"spacing": {
					"blockGap": "calc( var(--wp--custom--element-spacing) / 2)"
				},
				"typography": {
					"fontSize": "var:preset|font-size|x-large"
				}
			}
		},
